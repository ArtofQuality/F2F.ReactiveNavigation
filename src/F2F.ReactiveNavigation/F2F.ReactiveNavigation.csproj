<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageTargetFallback>$(PackageTargetFallback);portable-win+net45+wp8+win81+wpa8</PackageTargetFallback>
    <Version>4.0.0</Version>
    <Authors>Florian Fordermaier, Florian Schinagl</Authors>
    <Company>Florian Fordermaier, Florian Schinagl</Company>
    <Description>Region-based navigation on top of ReactiveUI and some extra goodies</Description>
    <Copyright>Copyright © Florian Fordermaier, Florian Schinagl 2015-2018</Copyright>
    <PackageLicenseUrl>https://github.com/F-2-F/F2F.ReactiveNavigation/blob/master/LICENSE.txt</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/F-2-F/F2F.ReactiveNavigation</PackageProjectUrl>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
  </PropertyGroup>
  <Import Project="..\..\.paket\Paket.Restore.targets" />
</Project>